{
    "manifest_version": 3,
    "name": "Readclip",
    "description": "Read article without ads widget or any distractions.",
    "version": "1.0",
    "action": {
      "default_popup": "main.html",
      "default_icon": "icons/icon128.png"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["activeTab", "scripting"]
}